page{
  background: #F2F2F2;
}
.info{
  background: #fff;
  padding: 30rpx;
  margin-bottom: 10rpx;
}
.banner{
  width:100%;
  height: 320rpx;
  margin: 0 auto 70rpx;
  border-radius: 10rpx;
}
.infoTitle{
  font-size:34rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:48rpx;
  margin-bottom: 10rpx;
}
.infoLabel{
  margin-bottom: 40rpx;
}
.infoLabel text{
  background:rgba(254,240,229,1);
  border-radius:4rpx;
  height: 30rpx;
  line-height: 30rpx;
  padding: 0 16rpx;
  font-size:20rpx;
  font-weight:400;
  color:rgba(150,113,65,1);
  margin: 0 20rpx 20rpx 0;
}
.infoText{
  font-size:24rpx;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height:34rpx;
  margin-bottom: 10rpx;
}
.volume{
  background: #fff;
  margin-bottom: 10rpx;
  padding: 20rpx 0 20rpx;
}

.title{
  font-size:28rpx;
  font-weight:400;
  color:#333333;
  line-height:34rpx;
  text-align: center;
  margin: 20rpx 30rpx 40rpx;
  position: relative;
}
.title::after{
  content: "";
  position: absolute;
  left: 0;
  top: 50%;
  height: 2rpx;
  width:260rpx;
  background: #000000;
  opacity:0.09720000000000001;
}
.title::before{
  content: "";
  position: absolute;
  right: 0;
  top: 50%;
  height: 2rpx;
  width:260rpx;
  background: #000000;
  opacity:0.09720000000000001;
}

.voucherList{
  width: 690rpx;
  height: 180rpx;
  margin: 0 auto 20rpx;
  position: relative;
}
.voucherList image{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.voucherText{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 5;
}
.voucherA{
  position: absolute;
  top: 0;
  left: 0;
  width: 168rpx;
  font-size:40rpx;
  font-weight:800;
  color:rgba(255,255,255,1);
  line-height:180rpx;
  text-align: center;
}
.voucherB{
  position: absolute;
  top: 0;
  left: 168rpx;
  right: 132rpx;
  bottom: 0;
  padding: 20rpx 18rpx;
}
.voucherB text{
  display: block;
  font-size:20rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:34rpx;
}
.voucherB text:nth-child(1){
  font-size:28rpx;
  font-weight:500;
  margin-bottom: 34rpx;
}
.voucherB text:nth-child(2){
  font-size:24rpx;
  font-weight:500;
  margin-bottom: 14rpx;
}
.voucherC{
  position: absolute;
  top: 0;
  right: 0;
  width: 132rpx;
  height: 100%;
  font-size:28rpx;
  font-weight:500;
  color:rgba(254,240,229,1);
  line-height:40rpx;
  text-align: center;
}
.voucherC text{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.voucherH text{
  width: 100%;
  text-align: center;
  color: #FFFFFF;
}

.code{
  background: #fff;
  margin-bottom: 10rpx;
  padding: 20rpx 0 20rpx;
}
.code image{
  display: block;
  width: 360rpx;
  height: 360rpx;
  margin: 0 auto;
  background: #ccc;
}
.detail{
  background: #fff;
  margin-bottom: 10rpx;
  padding: 20rpx 0 20rpx;
}
.content{
  font-size:24rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:34rpx;
  padding: 0 30rpx;
}