page{
  background: #f2f2f2;
}
.section, .pad30{
  background: #fff;
  padding: 30rpx;
  margin-bottom: 10rpx;
}

.pad30 {
  background: #fff;
}
.swiper{
  width: 100%;
  height: 320rpx;
  overflow: hidden;
  border-radius: 8rpx;
}
.swiper .img{
  width: 100%;
  height: 100%;
}
.list-wrapper{
  margin: 0;
}
.cell {
  height: auto;
  margin: 0 0 60rpx 0;
  background: #fff;
  overflow: hidden;
  position: relative;
  min-height: 200rpx;
  padding-left: 220rpx;
}

.cell .img{
  position: absolute;
  left: 0;
  top: 0;
  width: 200rpx;
  height: 200rpx;
}
.cell .detail{

}
.cell .tit{
  font-size:34rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:48rpx;
}
.cell .label{
  margin-bottom: 20rpx;
}
.cell .label text{
  font-size:20rpx;
  font-weight:400;
  color:rgba(150,113,65,1);
  background:rgba(254,240,229,1);
  border-radius:4rpx;
  line-height: 30rpx;
  padding: 0 16rpx;
  margin-right: 20rpx;
}
.cell .address{
  font-size:24rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:34rpx;
  margin-bottom: 20rpx;
}
.line{
  height:2rpx;
  background: rgba(238,238,238,1);
}
.cell .coucher{
}
.cell .coucher text{
  font-size:20rpx;
  font-weight:400;
  color:rgba(150,113,65,1);
  line-height:28rpx;
  margin-right: 30rpx;
}
.cell .coucher text text{
  margin-right: 10rpx;
  background: #967141;
  border-radius:4rpx;
  font-size:20rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  padding: 0 5rpx;
}
.cell .info{
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  margin-top: 30rpx;
}
.cell .cost{
  font-size: 28rpx;
  font-weight: 500;
  color: #333;
}
.cell .cost text{
  color: #666;
  font-size: 16rpx;
  font-weight: 400;
}
.cell .btn{
  width: 118rpx;
  height: 48rpx;
  line-height: 48rpx;
  margin: 0;
  font-size: 28rpx;
  border-radius: 4rpx; 
  color: #fff !important;
}