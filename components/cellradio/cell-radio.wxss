.cell-radio{
  width: 100%;
  height: inherit;
  line-height: inherit;
  background: inherit;
  display: inline-block;
  border-radius: 4rpx;
  border:none;
  background: #f5f5f5;
  transition: all .2s;
}
.cell-radio.checked{
  color: #fff;
  background:#967141; 
}
.cell-radio.bg{
  position: relative;
  background: #f5f5f5;
}
.cell-radio.bg.checked{
  background: #967141;
  color: #fff;
}
.cell-radio .bg{
  width: 100%;
  height: 100%;
  position: absolute;
  top:0;
  left:0;
  z-index:-1;
}